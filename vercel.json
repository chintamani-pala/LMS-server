{
  "version": 2,
  "name": "lms-server-chintamanipala",
  "builds": [
    {
      "src": "build/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
    // Add any other environment variables here
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
      // Add any other build-time environment variables here
    }
  }
}
