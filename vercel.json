{
  "version": 2,
  "name": "lms-server-chintamanipala",
  "builds": [
    {
      "src": "build/server.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/*": {
      "runtime": "node@18",
      "memory": 512,
      "maxDuration": 50
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
